{
  "compileOnSave": true,
  "compilerOptions": {
    "outDir": "./wwwroot/js",
    "rootDir": "./scripts",
    "module": "ES6",
    "moduleResolution": "Node",
    "strict": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "ES6"
  },
  "include": [
    "scripts/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
